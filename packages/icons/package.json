{
  "name": "@bootstrap-ai/icons",
  "version": "0.1.0",
  "description": "SVG icon set and icon web component for Bootstrap AI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./icons/*": {
      "import": "./dist/icons/*.js",
      "types": "./dist/icons/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc && npm run build:icons",
    "build:icons": "node scripts/build-icons.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "@bootstrap-ai/core": "*",
    "lit": "^3.1.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "keywords": [
    "icons",
    "svg",
    "web-components"
  ],
  "author": "Your Organization",
  "license": "MIT"
}
